description "Gunicorn application server running web_flask/0-hello_route.py"

start on runlevel [2345]
stop on runlevel [!2345]

respawn
#setuid user
setgid www-data

#env PATH=/home/ubuntu/web_flask/0-hello_route
chdir /home/ubuntu/AirBnB_clone_v2/web_flask
exec gunicorn --workers 3 --bind localHost:8000 0-hello_route:app
